{
  "name": "babel-plugin-import-asserts",
  "version": "1.0.0",
  "description": "babel plugin that adds console.asserts which check that your imports are not undefined",
  "author": {
    "name": "Hugo Dozois-Caouette"
  },
  "main": "lib/index.js",
  "directories": {
    "lib": "lib/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dozoisch/babel-plugin-import-asserts.git"
  },
  "scripts": {
    "test": "npm run lint && npm run build && npm run test-mocha",
    "test-mocha": "mocha --compilers js:babel/register",
    "build": "rm -rf lib && babel src --out-dir lib",
    "lint": "eslint ./",
    "patch": "release patch",
    "minor": "release minor",
    "major": "release major"
  },
  "peerDependencies": {
    "babel-core": ">=5.2.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-eslint": "^4.1.3",
    "eslint": "^1.5.0",
    "eslint-config-semistandard": "^5.0.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-standard": "^1.3.1",
    "mocha": "^2.3.2",
    "mt-changelog": "^0.6.2",
    "release-script": "^0.5.2"
  },
  "keywords": [
    "babel",
    "babel-plugin",
    "object",
    "assign",
    "extend",
    "polyfill"
